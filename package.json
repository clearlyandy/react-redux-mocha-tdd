{
  "name": "react-redux-mocha-tdd",
  "version": "1.0.0",
  "description": "React/Redux TDD, featuring Mocha and Istanbul",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "node_modules/.bin/mocha --watch --compilers js:babel-core/register ./src/**/*.spec.js --recursive --bail --ui bdd",
    "coverage": "node_modules/istanbul/lib/cli.js cover node_modules/.bin/_mocha --report html -x 'src/**/*.spec.js' -- --compilers js:babel-core/register ./src/**/*.spec.js --recursive --bail --ui bdd"
  },
  "keywords": [
    "react",
    "reactjs",
    "tdd",
    "mocha",
    "istanbul"
  ],
  "author": "Andy Hass <andyhass@me.com> (http://github.com/clearlyandy)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "istanbul": "^0.4.4",
    "jsdom": "^3.0.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-mock-store": "^1.1.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
